{
    "manifestVersion": 1,
    "id": "vsts-datadogevents",
    "name": "Datadog Event",
    "version": "0.1.10",
    "publisher": "WilliamMatheusBatista",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "An extension to publish events to Datadog.",
    "icons": {
        "default": "extension-icon.png"
    },
    "files": [
        {
            "path": "tasks/datadogevents"
        }
    ],
    "contributions": [
        {
            "id": "vsts-datadogevents",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/datadogevents"
            }
        }
    ],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "Continuous Integration",
        "Continuous Delivery",
        "Build",
        "Release",
        "Datadog"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://github.com/billbatista/vsts-datadogevents"
        },
        "issues": {
            "uri": "https://github.com/billbatista/vsts-datadogevents/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/billbatista/vsts-datadogevents"
    },
    "screenshots": [
        {
            "path": "tasks/datadogevents/screenshots/task.png"
        }
    ]
}